{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "bootstrap-darkmode",
  "version": "5.0.1",
  "description": "Stylesheet and Scripts for implementing dark mode with Bootstrap 4",
  "main": "dist/theme.js",
  "types": "dist/theme.d.ts",
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/clashsoft/bootstrap-darkmode.git"
  },
  "keywords": [
    "bootstrap",
    "bootstrap-4",
    "darkmode",
    "darktheme"
  ],
  "author": "Adrian Kunz",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/clashsoft/bootstrap-darkmode/issues"
  },
  "homepage": "https://github.com/clashsoft/bootstrap-darkmode#readme",
  "peerDependencies": {
    "bootstrap": ">=5"
  },
  "module": "fesm2015/bootstrap-darkmode.mjs",
  "es2020": "fesm2020/bootstrap-darkmode.mjs",
  "esm2020": "esm2020/bootstrap-darkmode.mjs",
  "fesm2020": "fesm2020/bootstrap-darkmode.mjs",
  "fesm2015": "fesm2015/bootstrap-darkmode.mjs",
  "typings": "bootstrap-darkmode.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./bootstrap-darkmode.d.ts",
      "esm2020": "./esm2020/bootstrap-darkmode.mjs",
      "es2020": "./fesm2020/bootstrap-darkmode.mjs",
      "es2015": "./fesm2015/bootstrap-darkmode.mjs",
      "node": "./fesm2015/bootstrap-darkmode.mjs",
      "default": "./fesm2020/bootstrap-darkmode.mjs"
    }
  },
  "dependencies": {
    "tslib": "^2.3.1"
  }
}